{
  "name": "spyre",
  "version": "1.0.0",
  "description": "Your visual inspiration, uninterrupted",
  "main": "index.js",
  "dependencies": {
    "@chakra-ui/core": "^0.5.2",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "next": "^9.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint-config-xo-react": "^0.22.0",
    "eslint-config-xo-typescript": "^0.26.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "typescript": "^3.7.5",
    "xo": "^0.25.3"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "extends": [
      "xo-react",
      "xo-typescript/space"
    ],
    "extensions": [
      "ts",
      "tsx"
    ],
    "rules": {
      "@typescript-eslint/quotes": [
        "error",
        "double"
      ],
      "@typescript-eslint/explicit-function-return-type": "off",
      "sort-imports": "off",
      "import/order": "off"
    }
  },
  "prettier": {
    "singleQuote": false,
    "bracketSpacing": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penumbra1/spyre.git"
